# Blockchain Configuration Example
# Copy this file to .env and replace with your actual values

# Your wallet private key (without 0x prefix)
# IMPORTANT: Never commit your actual private key!
WALLET_PRIVATE_KEY=your_private_key_here_without_0x_prefix

# Sepolia RPC URL (get one from Infura or Alchemy)
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your_project_id

# Infura Project ID (from your Infura dashboard)
INFURA_PROJECT_ID=your_infura_project_id

# NFT Contract Address (deployed on Sepolia)
NFT_CONTRACT_ADDRESS=0xYOUR_CONTRACT_ADDRESS_HERE

# Groq AI Configuration (for AI grading)
GROQ_API_KEY=your_groq_api_key_here
GROQ_MODEL=llama-3.3-70b-versatile
GROQ_API_URL=https://api.groq.com/openai/v1/chat/completions

# Pinata IPFS Configuration (for NFT storage)
PINATA_JWT=your_pinata_jwt_token_here
PINATA_API_KEY=your_pinata_api_key_here
PINATA_SECRET_KEY=your_pinata_secret_key_here

# Etherscan API Key (optional, for contract verification)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# SMTP Email Configuration
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_SECURE=tls
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_app_password_here
